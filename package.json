{
  "name": "ember-virtual-scrollkit",
  "version": "1.0.2",
  "description": "A library for using ember-collection with JS/CSS-based scrolling",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/yapplabs/ember-virtual-scrollkit",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "release": "release-it",
    "start": "ember server",
    "test": "ember try:testall"
  },
  "dependencies": {
    "ember-cli-babel": "^6.0.0",
    "ember-cli-htmlbars": "^1.1.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "ember-cli": "1.13.1",
    "ember-cli-app-version": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.0",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0",
    "ember-cli-ic-ajax": "0.2.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.15",
    "ember-cli-uglify": "^1.0.1",
    "ember-collection": "1.0.0-rc.0",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-disable-proxy-controllers": "^1.0.0",
    "ember-export-application-global": "^1.0.2",
    "ember-try": "0.0.6",
    "layout-bin-packer": "^1.5.0",
    "release-it": "^13.6.0",
    "release-it-lerna-changelog": "^2.3.0"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
